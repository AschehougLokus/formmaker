{% stylesheets 'bundles/formmaker/css/*' filter='cssrewrite' %}
    <link rel="stylesheet" href="{{ asset_url }}" />
{% endstylesheets %}

{% javascripts
    'extension/ezjscore/design/standard/javascript/jquery-ui-1.10.3.custom.min.js'
'@FormMakerBundle/Resources/public/js/*'
%}
    <script type="text/javascript" src="{{ asset_url }}"></script>
{% endjavascripts %}

<div id="formmaker_wrapper">
    <div id="formmaker_sidebar">
        {% block sidebar %}
            {% include 'FormMakerBundle::admin_left_menu.html.twig' %}
        {% endblock %}
    </div>

    <div id="formmaker_content">
        {% block content %}
        {% endblock %}
    </div>
</div>